package step02_연산자;

// casting : 자료형의 변환

public class CastingEx {

	public static void main(String[] args) {
		
		int k = 95, e = 85, m = 73;
		
		int tot = k + e + m;
		
		System.out.println("총점 : " + tot);
//		byte a = 100;
//		int b = a; // 자동 형변환
//
//		double c = 73.45;
//		int d = (int) c; // 강제 형변환
//		
//		System.out.println(a + "\t" + b);
//		System.out.println(c + "\t" + d);
	}

}
