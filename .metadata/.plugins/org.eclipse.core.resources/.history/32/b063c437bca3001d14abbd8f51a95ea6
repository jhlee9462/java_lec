package ex12;

import java.util.HashSet;

public class SetTest {

	public static void main(String[] args) {
		
		String str = args[0];
		HashSet<Character> hashSet = new HashSet<>();
		
		for (int i = 0; i < str.length(); i++) {
			hashSet.add(str.charAt(i));
		}
		
		for (char c : hashSet) {
			System.out.print(c + ", ");
		}
	}

}
